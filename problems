public class Main
{
	public static void main(String[] args) {
		int n[]={1,2,3,4,-1,-3};
		
		for(int i=0;i<n.length;i++){
		    int sum=0;
		    
		    for(int j=i;j<n.length;j++){
		        sum+=n[j];
		        if(sum==0){
		            for(int k=i;k<=j;k++){
		                System.out.print(n[k]+" ");
		            }
		          //  System.out.println();
		          //  sum=0;
		        }
		    }
		}
	}
}
