public class Main
{
	public static void main(String[] args) {
		
	int n=10;	
	int strt=(n*(n+1))/2,incr=2;
	for(int i=n;i>=1;i--){
		   for(int j=n;j>i;j--){
		       System.out.print("   ");
		    }
		    int dec=(n-i)+1 ,num=strt;
		    for(int j=0;j<i;j++){
		        System.out.print(num+" ");
		        num-=dec;
		        dec++;
		    }
		    strt-=incr;
		    incr++;
		    System.out.println();
		}
	}
}
