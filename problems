public class Main
{
	public static void main(String[] args) {
		
	int strt=1,incr,n=5;
      incr=n;
	for(int i=n;i>=1;i--){
		  for(int j=0;j<i;j++){
		        System.out.print("   ");
		    }
		    int dec=i,num=strt;
		    for(int j=i;j<=n;j++){
		        System.out.print(num+" ");
		        num-=dec;
		        dec++;
		    }
		    strt+=incr;
		    incr--;
		    System.out.println();
		}
	}
}
