public class Main
{
	public static void main(String[] args){
		int n=5,num=1,x,k,flag,strt=1,inc=2;
		
		for(int i=0;i<n;i++){
		    num=strt;
		    x=i+1;
		    
		    if(i==n-1) x=n-1;
		    
		    flag=0;
		    
		    for(k=0;k<n;k++){
		        System.out.printf("%2d ",num); //+" ");
		        num+=x;
		        
		        if(flag==1) x--;
		        else x++;
                
		        if(i==n-1 && flag==0){ //last row
		            x-=2;
		            flag=1;
		        } 
		        else if(x>n-1){
		            x--;
		            flag=1;
		        }
		    }
		    System.out.println();
		    strt+=inc;
		    inc++;
		}
	}
