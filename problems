public class Main
{
	public static void main(String[] args) {
	int n[]={22,33,44,65};
	int min=Integer.MAX_VALUE;
	for(int i=0;i<n.length-1;i++){
	    int diff= n[i+1]-n[i];
	    if(diff < min){
	        min = diff;
	    }
	}

	for(int i=0;i<n.length-1;i++){
	    if(n[i+1]-n[i] == min){
	        System.out.print("("+n[i]+","+n[i+1]+")");
	    }
	}
	
	}
}

