public class Main
{
	public static void main(String[] args) {
		int a[]={1,0,2,0,3,4,5,0};
		
		int k=0;
		
		for(int i=0;i<a.length;i++){
		    if(a[i]!=0){
		        a[k++]=a[i];
		    }
		}
		
		for(int i=k;i<a.length;i++){
		    a[i]=0;
		}
		
		for(int i=0;i<a.length;i++){
		    System.out.print(a[i]+" ");
		}
	}
}
