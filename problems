public class Main
{
	public static void main(String[] args) {
		int n=5,oddstrt=1,evenstrt=2,x=0;
		int oddincr=2,evenincr=4;
		int downodd=0,downeven=0;
		for(int i=0;i<n;i++){
		    for(int j=i;j<n;j++){
		        System.out.print(" ");
		    }
		    if(i%2==0){ //odd
		        x=oddstrt;
		          if(i==n-(n%2 == 0 ? 2 : 1)){
		            downodd=x;
		        }
		        for(int j=0;j<=i;j++){
		            System.out.print(x+" ");
		            if(j==i){
		                int y=x;
		                for(int k=0;k<=i;k++){
		                    System.out.print(y+" ");
		                    y-=2;
		                }
		            }
		            x+=2;
		        }
		          oddstrt+=oddincr;
		          oddincr+=4;
		    }
		    else{
		        x=evenstrt;
		             if(i==n-(n%2 == 0 ? 1 : 2)){
		            downeven=x;
		       
		        }
		        for(int j=0;j<=i;j++){
		            System.out.print(x+" ");
		            if(j==i){
		                int y=x;
		                for(int k=0;k<=i;k++){
		                    System.out.print(y+" ");
		                    y-=2;
		                }
		            }
		            x+=2;
		        }
		        evenstrt+=evenincr;
		        evenincr+=4;
		    }
		    System.out.println();
		}

//down Side
        evenincr-=8; 
	    oddincr-=8;
	boolean flag=(n%2==1);

		for(int i=0;i<n;i++){
		    for(int j=0;j<=i;j++){
		        System.out.print(" ");
		    }
		    
		    if(flag){ //odd
		        x=downodd;

		        for(int j=i;j<n;j++){
		            System.out.print(x+" ");
		            if(j==n-1){
		                int y=x;
		                for(int k=i;k<n;k++){
		                    System.out.print(y+" ");
		                    y-=2;
		                }
		            }
		            x+=2;
		        }
		          downodd-=oddincr;
		          oddincr-=4;
		    }
		    else{
		        x=downeven;

		        for(int j=i;j<n;j++){
		            System.out.print(x+" ");
		            if(j==n-1){
		                int y=x;
		                for(int k=i;k<n;k++){
		                    System.out.print(y+" ");
		                    y-=2;
		                }
		            }
		            x+=2;
		        }
		        downeven-=evenincr;
		        evenincr-=4;
		    }
		    flag=!flag;
		    System.out.println();   
		}
	}
}
