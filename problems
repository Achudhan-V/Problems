public class Main
{
	public static void main(String[] args) {
		int n=5,x=0;  //boolean flag;
		
		for(int i=1;i<=n;i++){
		  x=n-1;
		    for(int j=i;j<=n+i-1;j++){
		      System.out.print(((j>n) ? x-- : j) +" ");
		    }
		    System.out.println();
		}
	}
}
