Palindrome

public class Main
{
	public static void main(String[] args) {
		char[] a={'a','b','c','b','d'};
		
		for(int i=0;i<a.length;i++){
		    for(int j=i+1;j<a.length;j++){
		        if(a[i]==a[j] && isPal(a,i,j)){
		            for(int k=i;k<=j;k++){
		                System.out.print(a[k]);
		            }
		            return;
		        }
		    }
		}
	}
		static boolean isPal(char[] a,int i,int j){
		 while(i<j){
		     if(a[i++]!=a[j--])
		     return false;
		 }
		 
		 return true;
		}
	}
