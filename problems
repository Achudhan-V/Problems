		int k=0,j=a.length-1;
		for(int i=a.length-1;i>=0;i--){
		    if(a[i]!=0){		        
		        int temp=a[i]; /
		        a[i]=a[j];
		        a[j]=temp;	        
		        j--;
		    }
		
		}
