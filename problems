public class Main
{
	public static void main(String[] args) {
		int n=5,oddstrt=1,evenstrt=2,x=0;
		int oddincr=2,evenincr=4;
		for(int i=0;i<n;i++){
		    if(i%2==0){ //odd
		        x=oddstrt;
		        for(int j=0;j<=i;j++){
		            System.out.print(x+" ");
		            x+=2;
		        }
		          oddstrt+=oddincr;
		          oddincr+=4;
		    }
		    else{
		        x=evenstrt;
		        for(int j=0;j<=i;j++){
		            System.out.print(x+" ");
		            x+=2;
		        }
		        evenstrt+=evenincr;
		        evenincr+=4;
		    }
		    System.out.println();
		  
		    
		}
	}
}
